<!--pages/enroll/enroll.wxml-->
<view class="background">
  <view class="first">
    <input type="text" placeholder="用户名" placeholder-class="plass" class="input" type="number" bindinput="usernameInput"></input>
  </view>
  <view class="secound">
    <input type="text" placeholder="手机号" placeholder-class="plass" class="input" type="number" bindinput="phoneNumberInput"></input>
  </view>
  <view class="secound">
    <input type="text" placeholder="登录密码" placeholder-class="plass" class="input" type="password" bindinput="passwardInput"></input>
  </view>
  <view class="secound">
    <input type="text" placeholder="确认密码" placeholder-class="plass" class="input" type="password" bindinput="passwardInputAck"></input>
  </view>
  <view class="click">注册</view>
  <view class="cha" bindtap="signin">
    <text class="no">已有账号，点我登录</text>
  </view>
</view>
